{
  /*
The following identity settings need to be configured
before the project can be successfully executed.
For more info see https://aka.ms/dotnet-template-ms-identity-platform
*/
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "[Enter your azure domain]",
    "TenantId": "[Enter your Azure TenantId",
    "ClientId": "[Enter your ClientId from App Registration]"
  },
  "Graph": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": "user.read"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Storage": {
    "ConnectionString": "[Enter Storage Account connection string]",
    "QueueName": "[Enter default Queue name]"
  },
  "KeyVault": {
    "VaultUri": "https://mykeyvault.vault.azure.net/"
  },
  "ApplicationInsights": {
    "LogLevel": {
      "Microsoft": "Error"
    },
    "ConnectionString": "[Enter Application Insights Connection String]"
  },
  "AllowedHosts": "*",
  "AllowedClients": "[Enter Client ids separated by semicolons]"
}
